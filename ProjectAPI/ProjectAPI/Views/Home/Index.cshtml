@using ProjectAPI.Models
@model IEnumerable<Class1>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Customer Id</th>
            <th>Contact Name</th>
            <th>Country</th>
        </tr>
        @foreach (Class1 customer in Model)
        {
    <tr>
        <td>@customer.id</td>
        <td>@customer.name</td>
        <td>@customer.pro</td>
        <td>@customer.phone</td>
        <td>@customer.photo</td>
    </tr>
        }
    </table>
</body>
</html>